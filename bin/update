#!/usr/bin/env bash

# Variable definitions
. {ENV_LOCATION}


# Iterate over each host
for ...

    # Parse conf
    {MODULE_NAME}-parse-conf "$file"



    # Process
    echo "vagrant" | ansible-playbook -U vagrant -k "$ANSIBLE_PLAYBOOK" -i "$VBOX"

    # Clean conf
    {MODULE_NAME}-clean-conf

done